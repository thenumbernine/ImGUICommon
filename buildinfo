distName='ImGuiCommon'
distType='inc'
if platform == 'msvc' then
	dynamicLibs:insert(cwd..'/lib/imgui.lib')
	dynamicLibs:append{'user32.lib', 'imm32.lib'}
end
if including then
	include:insert(cwd..'/include')
	if platform == 'linux' then
		include:insert'/usr/include/SDL2'
	end
	if platform ~= 'msvc' then
		dynamicLibs:insert(cwd..'/lib/'..libPrefix..'imgui'..libSuffix)
	end
end
